{
  "suggest.autoTrigger": "none",
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "typescript",
    "typescriptreact",
    "json"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}
